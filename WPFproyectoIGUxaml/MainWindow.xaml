<Window x:Class="WPFproyectoIGUxaml.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFproyectoIGUxaml"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <GroupBox x:Name="groupBoxInput" Header="Entrada" Margin="58,108,555,108">
            <TextBox x:Name="textBox1" Margin="83,0,28,0" TextWrapping="Wrap" Text="TextBox" Height="25"/>
        </GroupBox>
        <GroupBox x:Name="groupBoxProcess" Header="Proceso" Margin="295,108,318,108"/>
        <GroupBox x:Name="groupBoxOutput" Header="Salida" Margin="539,108,74,108">
            <ListBox d:ItemsSource="{d:SampleData ItemCount=5}" Height="146" Margin="32,0,21,0"/>
        </GroupBox>
        <Button x:Name="signatureBtn" Content="Firma" HorizontalAlignment="Left" Margin="72,38,0,0" VerticalAlignment="Top" Background="#FFD25454" Foreground="White" Height="26" Width="61" Click="signatureBtn_Click"/>
        <Button x:Name="processBtn" Content="Proceso" HorizontalAlignment="Left" Margin="358,162,0,0" VerticalAlignment="Top" Background="#FFD25454" Foreground="White" Height="26" Width="61" Click="processBtn_Click"/>
    </Grid>
</Window>
